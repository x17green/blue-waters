{\n  "$schema": "https://turbo.build/schema.json",\n  "globalDependencies": ["**/.env.*local"],\n  "pipeline": {\n    "build": {\n      "dependsOn": ["^build"],\n      "outputs": [".next/**", "!.next/cache/**", "dist/**"]\n    },\n    "dev": {\n      "cache": false,\n      "persistent": true\n    },\n    "lint": {\n      "dependsOn": ["^lint"]\n    },\n    "test": {\n      "dependsOn": ["^build"],\n      "outputs": ["coverage/**"]\n    },\n    "db:migrate": {\n      "cache": false\n    },\n    "db:seed": {\n      "cache": false\n    },\n    "clean": {\n      "cache": false\n    }\n  }\n}